{
  "env": {
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-recommended",
    "prettier"
  ],
  "parser": "@babel/eslint-parser",
  "rules": {
    "semi-spacing": "warn",
    "arrow-parens": "warn",
    "comma-dangle": "warn",
    "requireConfigFile": false
  }
}